{
  "haste": {
    "defaultPlatform": "ios",
    "platforms": [
      "android",
      "ios"
    ],
    "providesModuleNodeModules": [
      "react",
      "react-native"
    ]
  },
  "moduleNameMapper": {
    "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
    "^[./a-zA-Z0-9$_-]+\\.(bmp|gif|jpg|jpeg|png|psd|svg|webp|ttf|mp4|obj)$": "RelativeImageStub"
  },
  "modulePathIgnorePatterns": [
    "<rootDir>/node_modules/react-native/Libraries/react-native/",
    "<rootDir>/node_modules/react-native/packager/"
  ],
  "preprocessorIgnorePatterns": [
    "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|exponent|@exponent/*)"
  ],
  "setupFiles": [
    "<rootDir>/node_modules/jest-exponent/src/setup.js"
  ],
  "testEnvironment": "node"
}
